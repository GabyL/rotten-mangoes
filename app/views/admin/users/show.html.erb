<table>
<th>First Name</th>
<th>Last Name</th>
<th>Email</th>
<th>Admin?</th>
<th>Edit User</th>
<th>Delete User </th>
  <% @users.per(10).each do |user|%>
    <tr>
      <td><%=user.firstname%></td>
      <td><%=user.lastname%></td>
      <td><%=user.email%></td>
      <td><%=user.admin%></td>
      <td><%= link_to "edit", edit_admin_user_path(user)%></td>
      <td><%= link_to "delete", admin_user_path(user), method: :delete, confirm: "You sure?" %></td>
    <tr>
  <%end%>
</table>

<%= paginate @users%>
